@model IEnumerable <Tut1.Models.Students>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
	<link href="~/Content/bootstrap.css" rel="stylesheet" />
    <title>Index</title>
</head>
<body>
    <div class="container"> 

		<table class="table table-responsive">
			<tr>
				<th>ID:</th>
				<th>First Name:</th>
				<th>Last Name:</th>
				<th>Age:</th>
				<th>Program:</th>
			</tr>
			<tbody>
				@if (Model != null)
				{
					foreach (var x in Model)
					{
						<tr>
							<td>@x.StudentID</td>
							<td>@x.FirstName</td>
							<td>@x.LastName</td>
							<td>@x.Age</td>
							<td>@x.Program</td>
						</tr>
					}
				}
			</tbody>
		</table>


    </div>
</body>
</html>
